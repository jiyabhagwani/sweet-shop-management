datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
  role     String
}

model Sweet {
  id       Int    @id @default(autoincrement())
  name     String
  category String
  price    Float
  quantity Int
}

